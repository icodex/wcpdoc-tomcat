<?xml version="1.0"  encoding="utf-8" standalone="yes" ?>
<conf>
	<!--1.自定义配置： 当前使用的样式，填themes的id；设置为NONE则不加载个性化样式，沿用版本样式或基础样式 : alwaysLoad，如果為true就每次取值都加載xml，如果為false就只在啟動時加載 -->
	<!-- ++++++++++++++++++++++++++++++++++++++<<<PC自定义配置>>>++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ -->
	<currentThemes alwaysLoad="true">
		<pc>style-succinct</pc>
		<mobile>version-mobile</mobile>
	</currentThemes>
	<themes id="style-succinct">
		<!-- 简洁风格主题 -->
		<page id="home-indexPage">web-succinct/home/succinct-index</page>
		<!--分类知识列表页面 -->
		<page id="type-knowlist">web-succinct/knowtype/succinct-index</page> 
		<!--分类问答列表页面 -->
		<page id="type-questionslist">web-succinct/fqatype/succinct-index</page>
	</themes>
	<themes id="classic-standard">
		<!-- 三益定制检索 -->
		<page id="search-pro">web-simple/lucene/searchProSanyi</page>
	</themes>
	<!-- ++++++++++++++++++++++++++++++++++++++<<<移动端自定义配置>>>++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ -->
	<themes id="version-mobile">
		<!-- 移动端配置 -->
		<page id="error-sys">web-mobile/error</page>
		<page id="home-indexPage">web-mobile/index</page>
		<page id="docKnow-viewPage">web-mobile/know/view</page>
		<page id="fileKnow-viewPage">web-mobile/file/view</page>
		<page id="urlKnow-viewPage">web-mobile/resource/view</page>
		<page id="type-knowlist">web-mobile/know/list</page>
		<!--分类问答浏览页面 -->
		<page id="type-questionslist">web-mobile/questions/list</page>
		<!--问答浏览页面 -->
		<page id="questions-viewPage">web-mobile/questions/view</page>
		<!--检索结果页面 -->
		<page id="search-resultPage">web-mobile/search/result</page>
		<!--附件查看页面 -->
		<page id="file-viewPage">web-mobile/file/fileView</page>
		<!--检索页面 -->
		<page id="search-homePage">web-mobile/search/result</page>
		<!-- 文档知识创建页面 -->
		<page id="resourceUrl-creatPage">web-simple/resourceUrl/creatMini</page>
	</themes>
