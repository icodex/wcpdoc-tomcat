	<!-- -->
	<!--2.系统固定配置 +++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++ -->
	<!-- -->
	<!-- -->
	<!-- <<<PC标准版>>> -->
	<themes id="version-sta">
		<!-- 标准版空则使用默认配置 -->
	</themes>
	<!-- <<<PC专业版>>> -->
	<themes id="version-pro">
		<!-- 专业版空则使用默认配置 -->
	</themes>
	<!-- <<<PC旗舰版>>> -->
	<themes id="version-ult">
		<!-- 站点首页 -->
		<page id="home-indexPage">web-wide/index</page>
	</themes>
	<!-- <<<默认>>> -->
	<!-- 默认配置，最终将被覆盖，如果没有被覆盖则使用该配置 -->
	<baseThemes>
		<!-- 默认配置，其他样式将覆盖该配置 -->
		<!-- 错误页面 -->
		<page id="error-sys">web-simple/error</page>
		<!-- 系统首页 -->
		<page id="home-indexPage">web-simple/index</page>
		<!-- 专题栏目页 -->
		<page id="home-special">web-wide/special/home</page>
		<!-- 专题具体信息页面 -->
		<page id="info-special">web-wide/special/special</page>
		<!-- 专题的修改首页 -->
		<page id="editHome-special">web-wide/special/editHome</page>
		<!-- 专题的维护知识列表 -->
		<page id="editDocs-special">web-wide/special/editDocs</page>
		<!-- 高级搜索页 <page id="search-pro">web-simple/lucene/searchPro</page> -->
		<page id="search-pro">web-simple/lucene/searchProSanyi</page>
		<!-- 文档知识浏览页面 -->
		<page id="docKnow-viewPage">web-simple/know/view</page>
		<!-- 资源知识浏览页面 -->
		<page id="fileKnow-viewPage">web-simple/webfile/webfile</page>
		<!-- 资源URL浏览页面 -->
		<page id="urlKnow-viewPage">web-simple/resourceUrl/view</page>
		<!--分类知识列表页面 -->
		<page id="type-knowlist">web-simple/type/type</page>
		<!--分类问答列表页面 -->
		<page id="type-questionslist">web-simple/question/index</page>
		<!--问答浏览页面 -->
		<page id="questions-viewPage">web-simple/question/view</page>
		<!--检索结果页面 -->
		<page id="search-resultPage">web-simple/lucene/searchResult</page>
		<!--检索页面 -->
		<page id="search-homePage">web-simple/lucene/search</page>
		<!--附件查看页面 -->
		<page id="file-viewPage">web-simple/webfile/file</page>
		<!-- 文档知识创建页面 -->
		<page id="resourceUrl-creatPage">web-simple/resourceUrl/creat</page>
	</baseThemes>
